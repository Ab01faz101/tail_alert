{
    "name": "ab01faz101/tail-alert",
    "description": "A lightweight and responsive alert package for Laravel Livewire, built with Tailwind CSS.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ab01faz101\\TailAlert\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abolfazl Qaederahmat",
            "email": "ar.rahmatyy@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "livewire/livewire": "^3.0",
        "laravel/framework": "^11.0 || ^12.0"
    },
    "keywords": ["livewire", "alert", "notification", "tailwind", "toast", "laravel"],
    "extra": {
        "laravel": {
            "providers": [
                "Ab01faz101\\TailAlert\\TailAlertServiceProvider"
            ]
        }
    }
}
